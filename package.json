{
  "name": "cmark-gfm",
  "version": "0.5.1",
  "description": "Node bindings to GitHub's GFM-enhanced fork of the cmark Markdown parser",
  "repository": "https://github.com/BinaryMuse/node-cmark-gfm",
  "main": "index.js",
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "build": "node-gyp rebuild",
    "test": "mocha test/helper.js test/*.test.js",
    "update-vendor": "./script/update-vendor",
    "prebuild-node": "prebuild -t 8.9.0 -t 9.4.0 -t 10.11.0 -t 11.9.0 --strip",
    "prebuild-node-ia32": "prebuild -t 8.9.0 -t 9.4.0 -a ia32 --strip",
    "prebuild-electron": "prebuild -t 2.0.0 -t 3.0.0 -t 4.0.4 -r electron --strip",
    "prebuild-electron-ia32": "prebuild -t 2.0.0 -t 3.0.0 -t 4.0.4 -r electron -a ia32 --strip",
    "upload": "node ./script/upload.js"
  },
  "keywords": [
    "markdown",
    "cmark",
    "commonmark",
    "gfm",
    "cmark-gfm"
  ],
  "author": "Michelle Tilley <michelle@michelletilley.net>",
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.4.0",
    "prebuild-install": "^5.2.4",
    "node-addon-api": "*"
  },
  "devDependencies": {
    "prebuild": "^8.1.2",
    "@alrra/travis-scripts": "^3.0.1",
    "chai": "^3.5.0",
    "chai-html": "^1.1.0",
    "dedent": "^0.6.0",
    "mocha": "^5.2.0",
    "node-gyp": "^3.8.0"
  }
}
